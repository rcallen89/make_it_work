<h1><%= @project.name %></h1>

<p>Material: <%= @project.material %></p>
<p>Challenge Theme: <%= @project.challenge.theme %></p>
<p>Number of Contestants: <%= @project.number_of_contestants %></p>
<p>Average Contestant Experience: <%= @project.contestant_average_exp %> years</p>

<%= form_tag "#{@project.id}/contestant_projects" do%>
  <%= label_tag :contestant_id %>
  <%= text_field_tag :contestant_id %>
  <%= submit_tag "Add Contestant"%>
<% end %>
